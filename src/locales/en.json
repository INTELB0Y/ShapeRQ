{
  "debug": {
    "fetch": {
      "try": {
        "success": {
          "message": "Request to ${url} returned status: ${response.status}",
          "data": "Fetched data: ${JSON.stringify(data, null, 2)}"
        },
        "error": {
          "message": "Request to ${url} failed with status: ${response.status}",
          "data": "Error: ${response.statusText}"
        }
      },
      "catch": {
        "error": "Error message:\n${err}"
      }
    },
    "warn": {
      "messageVar1": "Error on request.",
      "messageVar2": "Something went wrong."
    },
    "info": {
      "start": "Starting request...",
      "complete": "Request completed."
    },
    "log": {
      "start": "Logging started...",
      "debugMode": "Debug mode enabled."
    }
  }
}
